   "source": [
    "from typing import List\n",
    "\n",
    "class Solution:\n",
    "    def grayCode(self, n: int) -> List[int]:\n",
    "        q = [0]\n",
    "        for k in range(n):\n",
    "            q.extend([1<<k | code for code in q[::-1]])\n",
    "        return q"
   ]
  },
